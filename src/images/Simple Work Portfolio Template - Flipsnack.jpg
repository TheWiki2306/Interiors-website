        if (modal.classList.contains('show')) {
            window.popstateTriggered = true;

            $('#templatePreview').modal('hide');
            window.modalEntrypointUrl = null;
        } else if (window.location.href !== window.modalEntrypointUrl) {
            window.location.reload();
        }
    });
